{
  "name": "react-sass-starterkit",
  "version": "1.4.0",
  "description": "Frontend setup for react with sass and webpack",
  "private": true,
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack -p",
    "delete": "node bin/cli.js delete",
    "new": "node bin/cli.js new",
    "start": "node bin/server.js",
    "test": "mocha internals/test/.setup-test.js app/**/*.test.js",
    "test:single": "mocha internals/test/.setup-test.js",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:frojd/react-sass-starterkit.git"
  },
  "keywords": [
    "webpack",
    "sass",
    "react",
    "frontend",
    "boilerplate",
    "starterkit",
    "cli"
  ],
  "author": "Daniel Karlsson",
  "license": "MIT",
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.9.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.2.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  }
}
