{
  "name": "react-sass-starterkit",
  "version": "2.2.0",
  "description": "Frontend setup for react with sass and webpack",
  "private": true,
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack -p",
    "delete": "node bin/cli.js delete",
    "new": "node bin/cli.js new",
    "publish": "webpack && node bin/cli.js publish",
    "publish:prod": "webpack -p && node bin/cli.js publish",
    "publish:only": "node bin/cli.js publish",
    "publish:all": "webpack && node bin/cli.js publishAll",
    "publish:snippet": "webpack && node bin/cli.js publishSnippet",
    "start": "node bin/server.js",
    "test": "mocha internals/test/.setup-test.js app/**/*.test.js",
    "test:single": "mocha internals/test/.setup-test.js",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:frojd/react-sass-starterkit.git"
  },
  "keywords": [
    "webpack",
    "sass",
    "react",
    "frontend",
    "boilerplate",
    "starterkit",
    "cli",
    "staticgenerator"
  ],
  "author": "Daniel Karlsson",
  "license": "MIT",
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.28.4",
    "enzyme": "^2.7.1",
    "eslint": "^3.18.0",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-react": "^7.0.1",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "js-beautify": "^1.6.14",
    "jsdom": "9.12.0",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "react-addons-test-utils": "^15.4.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
