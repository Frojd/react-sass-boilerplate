{
  "name": "react-sass-starterkit",
  "version": "2.1.1",
  "description": "Frontend setup for react with sass and webpack",
  "private": true,
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack -p",
    "delete": "node bin/cli.js delete",
    "new": "node bin/cli.js new",
    "publish": "webpack && node bin/cli.js publish",
    "publish:prod": "webpack -p && node bin/cli.js publish",
    "publish:only": "node bin/cli.js publish",
    "start": "node bin/server.js",
    "test": "mocha internals/test/.setup-test.js app/**/*.test.js",
    "test:single": "mocha internals/test/.setup-test.js",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:frojd/react-sass-starterkit.git"
  },
  "keywords": [
    "webpack",
    "sass",
    "react",
    "frontend",
    "boilerplate",
    "starterkit",
    "cli",
    "staticgenerator"
  ],
  "author": "Daniel Karlsson",
  "license": "MIT",
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.2.2",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.27.3",
    "enzyme": "^2.7.1",
    "eslint": "^3.18.0",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-react": "^6.10.3",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.1",
    "postcss-loader": "^1.3.1",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.0",
    "webpack": "^2.3.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  }
}
